          %th{:colspan => 6}
            = "Personal scores for "
            - if @player.player_acc_type == "Reg"
              = " (Reg)"
            - else
              = image_tag "#{@player.player_acc_type}.png", style: "vertical-align: -6px;"
            - if @player.player_name == "PlNKlE"
              %span{:style => "color: #DF06B5"} PlNKlE
            - else
              = " #{@player.player_name}"
