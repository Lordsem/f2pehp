          - (["overall"] + F2POSRSRanks::Application.config.f2p_skills).each do |skill|
            %tr
              - skill_f2p_rank = @player.f2p_skill_rank(skill).to_i
              %th= link_to image_tag("skills/#{skill}.png"), ranks_path(skill: skill, show_limit: 25, page: (skill_f2p_rank.to_f / 25).ceil)
              %td= @player["#{skill}_lvl"]
              %td= number_with_delimiter(@player["#{skill}_xp"].to_i, :delimiter => ",") 
              %td= number_with_delimiter(@player["#{skill}_ehp"].to_f, :delimiter => ",", :separator => ".") 
              %td= number_with_delimiter(@player["#{skill}_rank"].to_i, :delimiter => ",") 
              %td= number_with_delimiter(skill_f2p_rank, :delimiter => ",")
          %tr
            %th= image_tag "skills/combat.png"
            %td= @player["combat_lvl"]
          %tr
            %td= ""
          %tr
            %td= ""
          %tr
            %td= ""
            
