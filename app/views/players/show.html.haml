          %th{:align => "center"} Clues
          %th{:align => "center"} Score
          %th{:align => "center"} Rank
          %th{:align => "center"} F2P Rank
        %tbody
          -# %tr
          -#   - clue_all_f2p_rank =  @player.f2p_clues_rank("all")
          -#   %th= link_to image_tag("skills/clue.png"), ranks_path(skill: "clues_all", show_limit: 25, page: (clue_all_f2p_rank.to_f / 25).ceil)
