      - elsif @player.player_acc_type == "UIM"
        = link_to "OSRS Hiscores", "http://services.runescape.com/m=hiscore_oldschool_ultimate/hiscorepersonal.ws?user1=#{@player.url_friendly_player_name}", target: "_blank", rel: "noopener noreferrer"
      - elsif @player.player_acc_type == "IM"
